<div class="container">
  <div style="text-align: center;">
    <h1>画像投稿フォーム</h1>
    <%= form_with model: @photo_image, local:true do |f| %>
    <h4>画像</h4>
      <%= f.attachment_field :image %>
    <h4>写真名</h4>
      <%= f.text_field :name %>
    <h4>写真の説明</h4>
      <%= f.text_area :caption %>
      <%= f.submit '投稿' %>
    <% end %>
  </div>
</div>
